digraph "StyleTest" {
    graph [rankdir=TB, bgcolor=white, fontname="helvetica", fontsize=12]
    node [fontname="helvetica", fontsize=12, penwidth=2, style=dashed]
    edge [fontname="helvetica", fontsize=12, color="#FF0000", style=dotted, penwidth=3, arrowhead=vee, arrowtail=diamond]

    a [label="Start", shape=ellipse, style="rounded,filled", fillcolor="#A3D1A3", fontcolor="black"]
    b [label="End", shape=doublecircle, style="rounded,filled", fillcolor="#FFA3A3", fontcolor="black"]

    a -> b

}