digraph "EdgeStyleTest" {
    graph [rankdir=TB, bgcolor=white, fontname=helvetica, fontsize=12]
    node [fontname=helvetica, fontsize=12]
    edge [fontname=helvetica, fontsize=12, color=blue, style=dashed]

    a [label="Start", shape=ellipse, style="filled", fillcolor="lightgreen", fontcolor="black"]
    b [label="Check", shape=diamond, style="filled", fillcolor="lightyellow", fontcolor="black"]
    c [label="End", shape=doublecircle, style="filled", fillcolor="lightcoral", fontcolor="black"]

    a -> b
    b -> c [label=" Yes"]

}