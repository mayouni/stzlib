digraph "SoxPayment" {
    graph [rankdir=TB, bgcolor=white, fontname="helvetica", fontsize=12]
    node [fontname="helvetica", fontsize=12]
    edge [fontname="helvetica", fontsize=12, color="#0000FF", style=solid]

    submit [label="Submit", shape=ellipse, style="rounded,filled", fillcolor="#008000", fontcolor="white"]
    approve [label="Approve?", shape=diamond, style="filled", fillcolor="#FFFF00", fontcolor="black"]
    pay [label="Pay", shape=box, style="rounded,filled", fillcolor="#0000FF", fontcolor="white"]
    log [label="Log", shape=box, style="rounded,filled", fillcolor="#808080", fontcolor="white"]
    done [label="Done", shape=doublecircle, style="rounded,filled", fillcolor="#008000", fontcolor="white"]

    submit -> approve
    approve -> pay [label=" Yes"]
    pay -> log
    log -> done

}